{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch index.html",
      "type": "firefox",
      "request": "attach",
      "pathMappings": [
        {
          "url": "http://127.0.0.1:5500",
          "path": "${workspaceFolder}"
        },
        {
          "url": "http://localhost:5500",
          "path": "${workspaceFolder}"
        }
      ]
    }
  ]
}